	"license": "MIT",
	"homepage": "https://docs.getrevox.com/n8n/quickstart",
	"description": "n8n community nodes for the Revox AI calling platform",
	"keywords": [
		"n8n-community-node-package",
		"revox",
		"ai",
		"voice",
		"calling",
		"n8n"
	],
	"author": {
		"name": "Revox",
		"email": "support@getrevox.com"
	},
	"repository": {
		"type": "git",
